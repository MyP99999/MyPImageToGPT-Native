{
  "expo": {
    "name": "mypimagetogpt-native",
    "slug": "mypimagetogpt-native",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash2.png",
      "resizeMode": "cover",
      "backgroundColor": "#1E2225"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier" : "com.myp99999.mypimagetogptnative"
    },
    "android": {
      "bundleIdentifier" : "com.myp99999.mypimagetogptnative",
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo.png",
        "backgroundColor": "#1E2225"
      },
      "package": "com.myp99999.mypimagetogptnative"
    },
    "web": {
      "favicon": "./assets/logo.png"
    },
    "scheme": "mypimagetogpt-native",
    "extra": {
      "eas": {
        "projectId": "cffd85ae-e7a7-40ed-ac18-541ae3bc1262"
      }
    },
    "plugins": [
      "@react-native-google-signin/google-signin",
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let the app to read the text."
        }
      ],
      "react-native-tesseract-ocr"
    ],
    "permissions": [
      "INTERNET"
    ]
  }
}
